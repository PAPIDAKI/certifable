<% provide(:title,@user.name)%>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
        <h1>
          <%= gravatar_for @user %>
          <%= @user.name%>
        </h1>
      <br>
      <%=@user.email%> 
      <br>
      Created:<%=@user.created_at%> 
      <br>
      Updated:<%=@user.updated_at%>
      <br>
      The time is <%=Time.now%>
    </section>
  </aside>
</div>
